<%= link_to '#', class: "btn btn-default", id: "print-ticket#{equipment_customer.id}"  do %>
  <i class="fa fa-fw fa-file-text-o"></i>
  Ticket
<% end %>


<script>
  $(document).ready(function() {
    $("#print-ticket<%= equipment_customer.id %>").click(function() {
      printPage($(this), '<%= url_report %>');
    });
  });
</script>
