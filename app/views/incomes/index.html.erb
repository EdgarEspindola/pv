<div class="row">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header text-center">
        <h3 class="box-title title-catalog">Ingresos</h3>
      </div>

      <div class="box-body table-responsive no-padding">
        <%= render 'shared/filters' %>
        <%#= render 'shared/search', id: 'search-incomes',
          placeholder: 'Buscar por nombre del cliente o folio',
          remote_url: '/incomes/search' %>
        <table class="table table-responsive table-hover table-condensed table-striped" id="incomes_table">
          <%= render 'body_incomes' %>
        </table>
      </div>

    </div>
  </div>
</div>

<%= render 'shared/paginate', elements_paginate: @incomes %>
