<div class="row">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header text-center">
        <h3 class="box-title title-catalog">Servicios en Proceso</h3>
      </div>

      <div class="box-body table-responsive no-padding">
        <%= render 'shared/search', id: 'search-pending-services',
          placeholder: 'Buscar por nombre',
          remote_url: '/incomes/pending_service_by_client' %>
        <table class="table table-responsive table-hover table-condensed table-striped" id="pending_services_table">
          <%= render 'body_pending_services' %>
        </table>
      </div>
    </div>
  </div>
</div>

<%= render 'shared/paginate', elements_paginate: @pending_services %>
