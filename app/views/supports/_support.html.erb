<div class="card">
  <div class="card-header" id="heading<%= support.id %>">
    <h5 class="mb-0">
      <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapse<%= support.id %>">
        <%= "[Fecha de Registro] #{support.created_at.to_formatted_s(:long)}" %>
      </button>
    </h5>
  </div>

  <div id="collapse<%= support.id %>" class="collapse" data-parent="#accordionExample">
    <div class="card-body">
      <p> <%= support.description %>
    </div>
  </div>
</div>
