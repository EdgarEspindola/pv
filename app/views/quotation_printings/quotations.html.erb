<div class="row">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-header text-center">
        <h3 class="box-title title-catalog">Ordenes de Trabajo</h3>
      </div>

      <div class="box-body table-responsive no-padding">
        <%= render 'shared/search', id: 'search-quotation-printings',
          placeholder: 'Buscar por nombre',
          remote_url: '/quotation_printings/all_by_client' %>
        <table class="table table-responsive table-hover table-condensed table-striped" id="quotation_printings_table">
          <%= render 'body_quotation_printings' %>
        </table>
      </div>
    </div>
  </div>
</div>

<%= render 'shared/paginate', elements_paginate: @quotation_printings %>
