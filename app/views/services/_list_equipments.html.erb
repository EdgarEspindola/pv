<div class="box">
  <div class="box-header text-center">
  	<h3 class="box-title">Equipos Ingresados</h3>
    <div class="box-tools pull-right">
      <% unless @service.paid %>
        <%= link_to new_equipment_customer_path(service_id: @service.id), class: 'link-hover', data: {remote: true, disable_with: 'Accediendo...'} do %>
          <i class="fa fa-fw fa-plus-square" style="font-size: 2.2rem;"></i>
          Abrir Registro
          <% end %>
      <% else %>
        <%= render 'shared/generate_report', url_report: generate_ticket_service_path(@service, format: 'pdf') %>
      <% end %>
    </div>
  </div>
  <div class="box-body" id="new-equipment-body">
    <%= render @service.equipment_customers.order(created_at: :desc) %>
  </div>
</div>

<script>
  var acc = document.getElementsByClassName("accordion");
  activeAcordionPanels(acc);  
</script>
